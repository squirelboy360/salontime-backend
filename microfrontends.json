{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "salontime-landing": {
      "development": {
        "fallback": "www.salontime.nl"
      }
    },
    "salontime-backend": {
      "routing": [
        {
          "paths": [
            "/api/:path*",
            "/webhook/:path*",
            "/auth/:path*",
            "/health"
          ]
        }
      ],
      "development": {
        "fallback": "www.salontime.nl"
      }
    }
  }
}
